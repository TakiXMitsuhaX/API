  "components": {
    "schemas": {
      "ScrapeRequest": {
        "type": "object",
        "properties": {
          "url": {
            "type": "string",
            "description": "URL of the web page to scrape"
          },
          "form": {
            "type": "object",
            "description": "Form data to post to the web page"
          }
        }
      },
      "Error": {
        "type": "object",
        "properties": {
          "message": {
            "type": "string",
            "description": "Error message"
          }
        }
      },
      "OCRRequest": {
        "type": "object",
        "properties": {
          "imageUrl": {
            "type": "string",
            "format": "uri",
            "description": "The URL of the image to extract text from."
          },
          "language": {
            "type": "string",
            "description": "The language to use for OCR.",
            "default": "eng"
          }
        },
        "required": [
          "imageUrl",
          "language"
        ]
      },
      "OCRResponse": {
        "type": "object",
        "properties": {
          "text": {
            "type": "string",
            "description": "The extracted text from the image."
          }
        }
      },
      "ErrorResponse": {
        "type": "object",
        "properties": {
          "error": {
            "type": "string",
            "description": "The error message."
          }
        }
      },
      "anime_full": {
        "type": "object",
        "properties": {
          "title": {
            "type": "string"
          },
          "description": {
            "type": "string"
          },
          "genres": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "rating": {
            "type": "number",
            "format": "float"
          }
        }
      },
      "422": {
        "type": "object",
        "properties": {
          "message": {
            "type": "string",
            "description": "A description of the error."
          },
          "animequote": {
            "type": "string"
          }
        }
      },
      "200": {
        "type": "object",
        "properties": {
          "data": {
            "type": "string",
            "description": "this code indicates the success"
          },
          "animequote": {
            "type": "string"
          }
        }
      }
    }
  },
  "paths": {
    "/scraper": {
      "get": {
        "tags": ["WebScraper"],
        "summary": "Scrape web content",
        "description": "Scrapes the content of a web page specified by the `url` query parameter.",
        "parameters": [
          {
            "name": "url",
            "in": "query",
            "description": "URL of the web page to scrape",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "text/plain": {
                "schema": {
                  "type": "string"
                }
              }
            }
          },
          "500": {
            "description": "Internal server error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Error"
                }
              }
            }
          }
        }
      }
    },
