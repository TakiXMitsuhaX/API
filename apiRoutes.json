          }
        }
      }, 
            "OCRRequest": {
        "type": "object",
        "properties": {
          "imageUrl": {
            "type": "string",
            "format": "uri",
            "description": "The URL of the image to extract text from."
          },
          "language": {
            "type": "string",
            "description": "The language to use for OCR.", 
            "default": "eng"
          }
        },
        "required": [
          "imageUrl", 
          "language"
        ]
      },
      "OCRResponse": {
        "type": "object",
        "properties": {
          "text": {
            "type": "string",
            "description": "The extracted text from the image."
          }
        }
      },
      "ErrorResponse": {
        "type": "object",
        "properties": {
